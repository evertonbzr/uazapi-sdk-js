{
  "compilerOptions": {
    "target": "ES2018",
    "module": "NodeNext",
    "lib": [
      "ES2020"
    ],
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
  }
}